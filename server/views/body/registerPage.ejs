<form action="http://localhost:5200/register" method="POST" class="regUserForm">
  <h1>registerUser</h1>
  <label
    >username
    <input type="text" name="username" required />
  </label>
  <label
    >First name
    <input type="text" name="firstName" />
  </label>
  <label>
    Last name
    <input type="text" name="lastName" />
  </label>
  <label
    >age
    <input type="number" name="age" />
  </label>
  <label
    >phone
    <input type="text" name="phone" />
  </label>
  <label
    >email
    <input type="email" name="email" />
  </label>
  <label
    >password
    <input type="password" name="password" />
  </label>
  <label
    >repeat password
    <input type="password" name="confirmPassword" />
  </label>
  <label>
    Shipping Address
    <div>
      <label
        >number
        <input type="text" name="shippingAddress.number" />
      </label>
      <label
        >City
        <input type="text" name="shippingAddress.city" />
      </label>
      <label
        >State
        <input type="text" name="shippingAddress.state" />
      </label>

      <label
        >Zip Code
        <input type="text" name="shippingAddress.zipCode" />
      </label>
    </div>

    <button type="submit">SUBMIT</button>
  </label>
</form>

<script>
  const form = document.querySelector("form");

  form.addEventListener("submit", (e) => {
    e.preventDefault();

    const data = {
      username: form.username.value,
      firstName: form.firstName.value,
      lastName: form.lastName.value,
      age: form.age.value,
      phone: form.phone.value,
      email: form.email.value,
      password: form.password.value,
      // shippingAddress: form.shippingAddress.value,
      // shippingAddressNumber: form.shippingAddress.number.value,
      // shippingAddressCity: form.shippingAddress[city].value,
      // shippingAddressState: form.shippingAddress[state].value,
      // shippingAddressZipCode: form.shippingAddress[zipCode].value,
    };

    console.log(data);
  });
</script>
